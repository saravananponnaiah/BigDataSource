

object ScalaBasics {
  def main(args: Array[String]): Unit = {
    println("Start Scala Program")
    
    var count=15
    count = 20
    
    val countVal = 100
    //countVal = 200
    
    if (count == 15)
    {
      println("Value of count = " + count)      
    }
    else
    {
      println("Value of count variable = " + count)
    }
    
    for(i <- 1 to 5) {
      println("Iteration : " + i)
    }
        
    
  }
}